<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pine.dao.mysql.CategoryDaoMapper">


    <select id="queryCategoryListByPid" parameterType="Long" resultType="List">
        SELECT  ca.name,ca.is_parent,ca.sort
         FROM   `tb_category` ca
        WHERE  parent_id = #{id}
    </select>
</mapper>
